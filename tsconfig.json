{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./"
    },
    "include": [
      "webpack/**/*.ts",
      "public/js/functions/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }